@model List<Producto>

@{
    ViewData["Title"] = "Listado de Productos";
}

<h2>@ViewData["Title"]</h2>

<table class="table table-striped">
    <thead class="table-dark">
        <tr>
            <th>ID</th>
            <th>Descripci√≥n</th>
            <th>Precio</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var p in Model)
        {
            <tr>
                <td>@p.IdProducto</td>
                <td>@p.Descripcion</td>
                <td>$@p.Precio</td>
            </tr>
        }
    </tbody>
</table>